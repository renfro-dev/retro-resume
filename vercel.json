{
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "outputDirectory": "dist/public",
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api" },
    { "source": "/vibetube/:videoId", "destination": "/api/vibetube/:videoId" },
    { "source": "/((?!assets/).*)", "destination": "/index.html" }
  ]
}
