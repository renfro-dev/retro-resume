{
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "outputDirectory": "dist/public",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api" },
    { "src": "/vibetube/([a-zA-Z0-9_-]+)", "dest": "/api/vibetube/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
